{
  "config": {
    "ignore_stop_words": true,
    "max_pattern_length": 1000,
    "max_queries": 500,
    "unit_name": "deterministic_intent_parser"
  },
  "group_names_to_slot_names": {
    "group0": "service"
  },
  "language_code": "fr",
  "patterns": {
    "rmesnard:getmessage": [
      "^\\s*message\\s*$",
      "^\\s*left\\s*message\\s*$"
    ],
    "rmesnard:sayno": [
      "^\\s*cancel\\s*$",
      "^\\s*negative\\s*$",
      "^\\s*no\\s*$"
    ],
    "rmesnard:sayyes": [
      "^\\s*affirmative\\s*$",
      "^\\s*yep\\s*$",
      "^\\s*yes\\s*$"
    ],
    "rmesnard:servicename": [
      "^\\s*hi\\s*(?P<group0>%VISITOR%)\\s*here\\s*$",
      "^\\s*(?P<group0>%VISITOR%)\\s*here\\s*$",
      "^\\s*hello\\s*(?P<group0>%VISITOR%)\\s*$",
      "^\\s*(?P<group0>%VISITOR%)\\s*$",
      "^\\s*this\\s*is\\s*(?P<group0>%VISITOR%)\\s*service\\s*$"
    ]
  },
  "slot_names_to_entities": {
    "rmesnard:getmessage": {},
    "rmesnard:sayno": {},
    "rmesnard:sayyes": {},
    "rmesnard:servicename": {
      "service": "visitor"
    }
  }
}